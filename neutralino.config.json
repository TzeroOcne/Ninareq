{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vite-project/dist/",
  "url": "/",
  "enableServer": true,
  "exportAuthInfo": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "filesystem.readDirectory"],
  "modes": {
    "window": {
      "title": "Ninareq",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vite-project/public/vite.svg",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "Ninareq",
    "resourcesPath": "/vite-project/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/vite-project/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
